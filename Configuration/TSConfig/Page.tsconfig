#-------------------------------------------------------------------------------
#	New Content Element Wizard
#-------------------------------------------------------------------------------
mod.wizards {
	newContentElement {
		wizardItems {
			t3sbsgrid {
				header = T3SB Grid
				elements {
					twocolumns {
						iconIdentifier = container-2col
						title = LLL:EXT:t3sbootstrap_grid/Resources/Private/Language/locallang_be.xlf:tx_container.twoColumns.title
						description = LLL:EXT:t3sbootstrap_grid/Resources/Private/Language/locallang_be.xlf:tx_container.twoColumns.description
						tt_content_defValues.CType = two_columns
					}
					threecolumns {
						iconIdentifier = container-3col
						title = LLL:EXT:t3sbootstrap_grid/Resources/Private/Language/locallang_be.xlf:tx_container.threeColumns.title
						description = LLL:EXT:t3sbootstrap_grid/Resources/Private/Language/locallang_be.xlf:tx_container.threeColumns.description
						tt_content_defValues.CType = three_columns
					}
					fourcolumns {
						iconIdentifier = container-4col
						title = LLL:EXT:t3sbootstrap_grid/Resources/Private/Language/locallang_be.xlf:tx_container.fourColumns.title
						description = LLL:EXT:t3sbootstrap_grid/Resources/Private/Language/locallang_be.xlf:tx_container.fourColumns.description
						tt_content_defValues.CType = four_columns
					}
					sixcolumns {
						iconIdentifier = container-4col
						title = LLL:EXT:t3sbootstrap_grid/Resources/Private/Language/locallang_be.xlf:tx_container.sixColumns.title
						description = LLL:EXT:t3sbootstrap_grid/Resources/Private/Language/locallang_be.xlf:tx_container.sixColumns.description
						tt_content_defValues.CType = six_columns
					}
					rowcolumns {
						iconIdentifier = container-3col
						title = LLL:EXT:t3sbootstrap_grid/Resources/Private/Language/locallang_be.xlf:tx_container.rowColumns.title
						description = LLL:EXT:t3sbootstrap_grid/Resources/Private/Language/locallang_be.xlf:tx_container.rowColumns.description
						tt_content_defValues.CType = row_columns
					}

				}
				show := addToList(twocolumns, threecolumns, fourcolumns, sixcolumns, rowcolumns)
			}
		}
	}
}
